{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {"url": "data/.json", "format": {"type": "json", "property": "data"}, "name": "data"},
    "title": "Messages sent daily in ",
    "width": 1024,
    "height": 512,
    "autosize": "fit",
    "config": {
        "font": "Comic Sans MS",
        "axis": {
            "gridOpacity": 0.5
        }
    },
    "mark": "line",
    "encoding": {
    "x": {"field": "date", "type": "temporal", "title": "Date"},
    "y": {"field": "count", "type": "quantitative", "title": "Messages per day"},
    "color": {"value": "#af3030"}
    }
  }
  